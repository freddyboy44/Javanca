            <script src="/theme/javanca/js/owl.carousel.min.js"></script>
            <script type="text/javascript" src="/theme/javanca/js/owl.carousel.min.js"></script> 
            <script type="text/javascript" src="/theme/javanca/js/bw.js"></script>     
            <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>
        {% endblock %}
    

    
